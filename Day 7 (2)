
#include <climits>
using namespace std;
int main() {
    // Write C code here
   int n;
   cin>>n;
   int arr[n];
   int max=INT_MIN,min=INT_MAX;
   
   for(int i=0;i<n;i++)
   {
       cin>>arr[i];
       if(arr[i]>max)
       {
           max=arr[i];
       }
       if(arr[i]<min)
       {
           min=arr[i];
       }
   }
   
   
  cout<<"maximum element in array is:"<<max<<endl;
  cout<<"Minimum element in array is:"<<min<<endl;
  
}
